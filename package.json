{
  "name": "turborepo-shadcn-ui",
  "version": "1.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "dotenv -- turbo build",
    "dev": "dotenv -- turbo dev",
    "prepare": "husky && dotenv -- turbo prepare",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "pnpm --filter @peanut/ui ui:add"
  },
  "devDependencies": {
    "@peanut/eslint-config": "workspace:*",
    "@peanut/typescript-config": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "husky": "^9.1.6",
    "prettier": "^3.3.2",
    "turbo": "2.0.6"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
